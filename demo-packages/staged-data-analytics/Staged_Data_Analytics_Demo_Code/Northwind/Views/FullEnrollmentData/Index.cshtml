@model IEnumerable<Northwind.Models.FullEnrollmentData>
@{
    ViewData["Title"] = "Northwind Insurance";
    ViewData["activePage"] = "azurestackdata";
}
<!DOCTYPE html>
<html>
<head>
    <style>
        body {
            background: none;
            background-color: #0d8ebd;
        }

        #fulldata {
            margin-top: 140px;
            padding-top: 40px;
            padding-left: 20px;
            padding-right: 20px;
            padding-bottom: 80px;
            background-color: white;
        }

        #fulldatatable {
            padding-top: 20px;
            padding-left: 20px;
            padding-right: 20px;
            overflow-x: scroll;
            background-color: #f7f7f7;
        }

        table {
            background-color: white;
            padding-left: 20px;
            padding-right: 20px;
        }

        thead {
            background-color: #0d8ebd;
            color: white;
        }
    </style>
    <meta name="viewport" content="width=device-width" />
    <title>AzureStack Data</title>
</head>
<body>
    <section id="fulldata" class="fulldata">
        <div id="fulldatatable" class="fulldatatable">
            <table class="table" border="1" cellpadding="3" align="center">
                <thead>
                    <tr>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.EnrollmentID)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.InsurancePlanID)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.SubscriberID)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.PlanYear)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.ConfirmationCode)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.TimeStamp)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.AddressLine1)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.AddressLine2)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.AlimonyChildSupport)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.City)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.County)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.EmailAddress)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.EmploymentIncome)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.FirstName)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.MiddleName)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.LastName)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.InvestmentIncome)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.IsMilitary)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.IsOnDisability)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.IsOnMedicare)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.IsStudent)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.IsUSCitizen)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.PhoneNumber)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.SocialSecurityNumber)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.State)
                        </th>
                        <th align="center">
                            @Html.DisplayNameFor(model => model.ZipCode)
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                {
                    <tr>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.EnrollmentID)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.InsurancePlanID)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.SubscriberID)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.PlanYear)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.ConfirmationCode)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.TimeStamp)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.AddressLine1)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.AddressLine2)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.AlimonyChildSupport)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.City)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.County)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.EmailAddress)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.EmploymentIncome)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.FirstName)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.MiddleName)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.LastName)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.InvestmentIncome)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.IsMilitary)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.IsOnDisability)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.IsOnMedicare)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.IsStudent)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.IsUSCitizen)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.PhoneNumber)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.SocialSecurityNumber)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.State)
                        </td>
                        <td align="center">
                            @Html.DisplayFor(modelItem => item.ZipCode)
                        </td>
                    </tr>
            }
                </tbody>
            </table>
            </div>
        </section>
</body>
</html>
